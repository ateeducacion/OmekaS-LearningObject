{
    "name": "ateeducacion/omeka-s-learning-object-adapter",
    "description": "Learning Object media ingester and renderer for Omeka S",
    "type": "omeka-s-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Área de Tecnología Educativa",
            "email": "ate.educacion@gobiernodecanarias.org"
        }
    ],
    "homepage": "https://github.com/ateeducacion/OmekaS-LearningObject",
    "support": {
        "issues": "https://github.com/ateeducacion/OmekaS-LearningObject/issues"
    },
    "keywords": [
        "Omeka S",
        "module",
        "learning object",
        "SCORM",
        "e-learning",
        "education"
    ],
    "require": {
        "php": ">=7.4"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "omeka/composer-addon-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*",
        "zerocrates/extract-tagged-strings": "dev-master",
        "laminas/laminas-form": "^3.4",
        "laminas/laminas-servicemanager": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LearningObjectAdapter\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LearningObjectAdapterTest\\": "test/"
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/zerocrates/extract-tagged-strings.git"
        }
    ]
}
